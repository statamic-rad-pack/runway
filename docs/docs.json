{
    "$schema": "https://mintlify.com/docs.json",
    "theme": "mint",
    "name": "Runway",
    "colors": {
        "primary": "#6F100B",
        "light": "#FF2B20",
        "dark": "#FF2B20"
    },
    "favicon": "/favicon.png",
    "navigation": {
        "tabs": [
            {
                "tab": "Documentation",
                "groups": [
                    {
                        "group": "Getting Started",
                        "pages": [
                            "index",
                            "installation",
                            "upgrade-guides",
                            "resources"
                        ]
                    },
                    {
                        "group": "Features",
                        "pages": [
                            "blueprints",
                            "control-panel",
                            "relationships",
                            "frontend-routing",
                            "templating",
                            "graphql",
                            "rest-api",
                            "search",
                            "revisions"
                        ]
                    },
                    {
                        "group": "Knowledge Base",
                        "pages": [
                            "kb-articles/multisite"
                        ]
                    }
                ]
            }
        ]
    },
    "logo": {
        "light": "/logo/light.svg",
        "dark": "/logo/dark.svg"
    },
    "navbar": {
        "links": [
            {
                "label": "GitHub",
                "href": "https://github.com/statamic-rad-pack/runway",
                "icon": "github"
            },
            {
                "label": "Statamic Marketplace",
                "href": "https://statamic.com/addons/rad-pack/runway",
                "icon": "/images/statamic-mark.svg"
            }
        ]
    },
    "contextual": {
        "options": [
            "copy",
            "view",
            "chatgpt",
            "claude",
            "perplexity",
            "mcp",
            "cursor",
            "vscode"
        ]
    },
    "footer": {
        "socials": {
            "github": "https://github.com/statamic-rad-pack/runway"
        }
    },
    "redirects": [
        {
            "source": "/kb-articles/supported-fieldtypes",
            "destination": "/blueprints"
        },
        {
            "source": "/fieldtypes",
            "destination": "/relationships"
        }
    ]
}
